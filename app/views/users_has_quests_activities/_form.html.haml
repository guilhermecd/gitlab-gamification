= form_for @users_has_quests_activity do |f|
  - if @users_has_quests_activity.errors.any?
    #error_explanation
      %h2= "#{pluralize(@users_has_quests_activity.errors.count, "error")} prohibited this users_has_quests_activity from being saved:"
      %ul
        - @users_has_quests_activity.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :quests_activities_id
    = f.number_field :quests_activities_id
  .field
    = f.label :users_id
    = f.number_field :users_id
  .actions
    = f.submit 'Save'
